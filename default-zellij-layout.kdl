layout {
    tab name="Terminal" {
      pane size=1 {
          plugin location="tab-bar"
      }
      pane
    }
    tab name="Code" {
      pane size=1 {
          plugin location="tab-bar"
      }
      pane {
        command "nvim"
      }
    }
    tab name="AI Agent" {
      pane size=1 {
          plugin location="tab-bar"
      }
      pane
    }
    tab name="Misc" {
      pane size=1 {
          plugin location="tab-bar"
      }
      pane
    }
}
